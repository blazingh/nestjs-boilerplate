{
  "name": "scrapper",
  "version": "0.0.3",
  "description": "description example",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier-standard '{src,apps,libs,test}/**/*.{ts,js,json,html,css}'",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "git-cz"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "9.4.1",
    "@nestjs/config": "2.3.2",
    "@nestjs/core": "9.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "9.4.1",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/testing": "9.4.1",
    "@types/bcrypt": "^5.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "bcrypt": "5.1.0",
    "buffer": "6.0.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "husky": "8.0.3",
    "ini": "^4.1.1",
    "inquirer": "^9.2.4",
    "ioredis": "^5.3.2",
    "module-alias": "2.2.2",
    "mongodb": "5.5.0",
    "mongoose": "^7.1.1",
    "npm-check-updates": "^16.10.12",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "^7",
    "swagger-ui-express": "4.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@types/express": "4.17.17",
    "@types/ini": "^1.3.31",
    "@types/jest": "29.5.1",
    "@types/node": "20.2.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/superagent": "4.1.17",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "prettier-standard": "^16.4.1",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleNameMapper": {
      "^@shared(.*)$": "<rootDir>/components/shared$1",
      "^@components(.*)$": "<rootDir>/components$1",
      "^@filters(.*)$": "<rootDir>/filters$1",
      "^@guards(.*)$": "<rootDir>/guards$1",
      "^@interfaces(.*)$": "<rootDir>/interfaces$1",
      "^@pipes(.*)$": "<rootDir>/pipes$1",
      "^@dto(.*)$": "<rootDir>/dto$1",
      "^@__types__(.*)$": "<rootDir>/__types__$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@shared": "./dist/components/shared",
    "@components": "./dist/components",
    "@filters": "./dist/filters",
    "@guards": "./dist/guards",
    "@interfaces": "./dist/interfaces",
    "@pipes": "./dist/pipes",
    "@dto": "./dist/dto",
    "@__types__": "./dist/__types__"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
